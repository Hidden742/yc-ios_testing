<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>en</string>
	<key>CFBundleDisplayName</key>
	<string>Youth Club</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>Youth Club</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0.6</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>6</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UIViewControllerBasedStatusBarAppearance</key>
	<false/>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	
	<!-- Firebase Configuration -->
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
		<key>NSAllowsArbitraryLoadsForMedia</key>
		<true/>
		<key>NSAllowsArbitraryLoadsInWebContent</key>
		<true/>
	</dict>
	
	<!-- Privacy Permissions -->
	<key>NSCameraUsageDescription</key>
	<string>Youth Club would like to access your camera to let you share photos with the community.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Youth Club would like to access your photo library to let you share images with the community.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Youth Club would like to access your microphone for voice messages and calls.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Youth Club would like to access your location to help you find nearby events and groups.</string>
	<key>NSUserNotificationsUsageDescription</key>
	<string>Youth Club would like to send you notifications about events, messages, and inspiration.</string>
	
	<!-- URL Schemes for Authentication -->
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>com.youthclub.app</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>youthclub</string>
			</array>
		</dict>
		<dict>
			<key>CFBundleURLName</key>
			<string>REVERSED_CLIENT_ID</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>com.googleusercontent.apps.123456789-abcdefghijklmnop</string>
			</array>
		</dict>
	</array>
	
	<!-- iOS 14+ Privacy -->
	<key>NSUserTrackingUsageDescription</key>
	<string>This allows Youth Club to provide you with personalized content and better experience.</string>
</dict>
</plist>
